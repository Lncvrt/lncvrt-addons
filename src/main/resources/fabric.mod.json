{
  "schemaVersion": 1,
  "id": "lncvrt-addons",
  "version": "${version}",
  "name": "Lncvrt Addons",
  "description": "Misc features & addons for Meteor Client",
  "authors": [
    "Lncvrt"
  ],
  "contact": {
    "issues": "https://github.com/Lncvrt/lncvrt-addons/issues",
    "sources": "https://github.com/Lncvrt/lncvrt-addons",
    "discord": "https://lncvrt.xyz/discord"
  },
  "icon": "assets/lncvrt-addons/icon.png",
  "environment": "client",
